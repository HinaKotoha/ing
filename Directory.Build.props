<?xml version="1.0" encoding="utf-8"?>
<Project>

  <PropertyGroup>
    <BasePath>$(MSBuildThisFileDirectory)</BasePath>
    <MinimumVisualStudioVersion>16.0</MinimumVisualStudioVersion>
    <Features>strict</Features>
  </PropertyGroup>

  <PropertyGroup>
    <PortableFramework>netstandard2.0</PortableFramework>
    <DesktopFramework>net472</DesktopFramework>
    <DesktopLatestFramework>net472</DesktopLatestFramework>
    <CoreFramework>netcoreapp3.0</CoreFramework>
  </PropertyGroup>

  <PropertyGroup Condition="'$(MSBuildProjectExtension)' == '.csproj'">
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(MSBuildProjectExtension)' == '.vbproj'">
    <LangVersion>16.0</LangVersion>
    <OptionExplicit>On</OptionExplicit>
    <OptionInfer>On</OptionInfer>
    <OptionStrict>On</OptionStrict>
    <OptionCompare>Binary</OptionCompare>
  </PropertyGroup>

</Project>
