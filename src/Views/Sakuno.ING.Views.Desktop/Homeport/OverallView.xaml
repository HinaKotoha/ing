<rxui:ReactiveUserControl x:Class="Sakuno.ING.Views.Desktop.Homeport.OverallView"
                          x:TypeArguments="viewmodel:OverallViewModel"
                          xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                          xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                          xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                          xmlns:rxui="http://reactiveui.net"
                          xmlns:viewmodel="clr-namespace:Sakuno.ING.ViewModels.Homeport;assembly=Sakuno.ING.ViewModels"
                          mc:Ignorable="d">
    <StackPanel>
        <TextBlock>
            <Run Text="Ship:" />
            <Run Name="ShipCount" />/<Run Name="MaxShipCount" />
        </TextBlock>
        <TextBlock>
            <Run Text="SlotItem:" />
            <Run Name="SlotItemCount" />/<Run Name="MaxSlotItemCount" />
        </TextBlock>
    </StackPanel>
</rxui:ReactiveUserControl>
